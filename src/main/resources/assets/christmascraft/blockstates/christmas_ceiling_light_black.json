{
  "multipart": [
    {
      "apply": {
        "model": "christmascraft:block/christmas_ceiling_light_black"
      },
      "when": {
        "facing": {
          "up": "true",
          "down": "true"
        }
      }
    },
    {
      "apply": {
        "model": "christmascraft:block/christmas_ceiling_light_black_side"
      },
      "when": {
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "christmascraft:block/christmas_ceiling_light_black_side",
        "y": 180
      },
      "when": {
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "christmascraft:block/christmas_ceiling_light_black_side",
        "y": 270
      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "christmascraft:block/christmas_ceiling_light_black_side",
        "y": 90
      },
      "when": {
        "facing": "east"
      }
    }
  ]
}
